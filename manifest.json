{
  "manifest_version": 3,
  "name": "Pixel Profit Calculator",
  "version": "1.0",
  "description": "Calculate profits based on PIXEL coin price",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "https://api.binance.com/"
  ]
}